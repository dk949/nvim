#!/usr/bin/python
class rgb:
    r = 0
    g = 0
    b = 0
    def __init__(self,r,g,b):
        self.r = r;
        self.g = g
        self.b = b

    def __str__(self):
        return f"rgb({self.r}, {self.g}, {self.b})"

