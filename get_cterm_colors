def adjust_hex(val):
    return f"#{str(hex(val))[2:].upper().rjust(6, '0')}"

def adjust(val, by, start='|', end=','):
    return f"{start}{val}".ljust(by)

print(
        adjust('Name', 19),
        adjust('cterm', 6),
        adjust('hex', 8),
        adjust('RGB', 19, end=''),
        end="|\n"+ ('-' * (19 + 7 + 7 + 19+6)) + '\n'
        )

for [name, cterm, hex_val, rgb_val] in text_cterm_gui_rgb:
    fname = adjust(name, 19)
    fcterm = adjust(cterm, 6)
    fhex_val = adjust(adjust_hex(hex_val), 7)
    frgb_val = adjust(rgb_val, 19, end='') + '|'
    print(
            fname,
            fcterm,
            fhex_val,
            frgb_val
            )
